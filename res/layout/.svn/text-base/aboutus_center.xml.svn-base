<LinearLayout
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" xmlns:android="http://schemas.android.com/apk/res/android">
    <RelativeLayout 
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="top"
        android:background="@drawable/title">
        <ImageButton 
            android:id="@+id/aboutus_center_imagebutton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="5dp"
            android:background="@drawable/imagebutton_back"></ImageButton>
        <TextView 
            android:id="@+id/aboutus_center_textview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="设置"
            android:textColor="#ffffff"
            android:textSize="18dp"></TextView>
    </RelativeLayout>
   <RelativeLayout 
       android:layout_width="fill_parent"
       android:layout_height="fill_parent"
       android:background="@drawable/backgroound">
       <ImageView 
           android:id="@+id/aboutus_center_imageview"
           android:layout_width="fill_parent"
           android:layout_height="wrap_content"
           android:background="@drawable/title_bottom"
           android:layout_alignParentTop="true"></ImageView>
       <ImageView
           android:id="@+id/aboutus_center_imageview2"
           android:layout_width="80dp"
           android:layout_height="80dp"
           android:layout_centerHorizontal="true"
           android:scaleType="fitXY"
           android:layout_marginTop="30dp"
           ></ImageView>
        <ImageView
           android:id="@+id/aboutus_center_imageview3"
           android:layout_width="80dp"
           android:layout_height="80dp"
           android:layout_centerHorizontal="true"
           android:scaleType="fitXY"
           android:layout_marginTop="30dp"
           android:src="@drawable/head_info"></ImageView>
        <RelativeLayout 
       android:layout_width="fill_parent"
       android:layout_height="wrap_content"
       android:layout_marginTop="140dp">
       <com.starbaby_03.view.TextviewView 
           android:id="@+id/aboutus_center_textview2"
           android:layout_width="fill_parent"
           android:layout_height="40dp"
           android:background="@drawable/info_shape"
           android:layout_marginLeft="30dp"
	       android:layout_marginRight="30dp"
	       android:layout_alignParentTop="true"></com.starbaby_03.view.TextviewView>
        <com.starbaby_03.view.TextviewView 
           android:id="@+id/aboutus_center_textview3"
           android:layout_width="fill_parent"
           android:layout_height="40dp"
           android:background="@drawable/info_shape2"
           android:layout_marginLeft="30dp"
	       android:layout_marginRight="30dp"
	       android:layout_below="@+id/aboutus_center_textview2"
	       android:layout_marginTop="-2dp"></com.starbaby_03.view.TextviewView>
        <com.starbaby_03.view.TextviewView 
           android:id="@+id/aboutus_center_textview4"
           android:layout_width="fill_parent"
           android:layout_height="40dp"
           android:background="@drawable/info_shape4"
           android:layout_marginLeft="30dp"
	       android:layout_marginRight="30dp"
	       android:layout_below="@+id/aboutus_center_textview3"
	       android:layout_marginTop="30dp"></com.starbaby_03.view.TextviewView>
       </RelativeLayout>
       <Button 
  	    android:id="@+id/aboutus_center_button1"
  	    android:layout_width="wrap_content"
  	    android:layout_height="wrap_content"
  	    android:text="退出登录"
  	    android:layout_alignParentBottom="true"
  	    android:textColor="#ffffff"
  	    android:textSize="15dp"
  	   	android:layout_marginBottom="40dp"
  	   	android:layout_centerHorizontal="true"
  	    android:background="@drawable/button_back"></Button>
   </RelativeLayout>
  	

</LinearLayout>